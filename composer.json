{
    "name": "spiral/app-keeper",
    "type": "project",
    "license": "MIT",
    "description": "Spiral Skeleton Application - Keeper",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/app-keeper/issues",
        "source": "https://github.com/spiral/app-keeper"
    },
    "authors": [
        {
            "name": "Wolfy-J",
            "email": "wolfy.jd@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "cycle/annotated": "^2.0",
        "cycle/migrations": "^1.0",
        "cycle/orm": "^1.0",
        "cycle/proxy-factory": "^1.0",
        "fzaninotto/faker": "^1.9",
        "spiral/database": "^2.3",
        "spiral/framework": "^2.7",
        "spiral/helpers": "^1.0",
        "spiral/jobs": "^2.0",
        "spiral/keeper": "^0.8",
        "spiral/migrations": "^2.0",
        "spiral/nyholm-bridge": "^1.0",
        "spiral/roadrunner": "^1.4",
        "spiral/toolkit": "^v1.1",
        "spiral/writeaway": "^0.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"copy('.env.sample', '.env');\"",
            "php app.php encrypt:key -m .env",
            "php app.php configure -vv",
            "spiral get-binary"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "publish-cmd": "php app.php publish"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
